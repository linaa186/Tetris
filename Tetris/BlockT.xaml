<UserControl x:Class="Tetris.BlockT"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Tetris"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="60">
    <Canvas x:Name="rightL" Width="60" Height="60">
        <Rectangle Width="20" Height="20" Fill="Green" Canvas.Left="20" Canvas.Top="40"/>
        <Rectangle Width="20" Height="20" Fill="Green" Canvas.Left="20" Canvas.Top="0"/>
        <Rectangle Width="20" Height="20" Fill="Green" Canvas.Left="20" Canvas.Top="20"/>
        <Rectangle Width="20" Height="20" Fill="Green" Canvas.Left="40" Canvas.Top="20"/>
    </Canvas>
</UserControl>
